{
    "mappings": {
        "doc": {
            "properties": {
                "@timestamp"    : {"type": "date"},
                "@version"      : {"type": "keyword"},
                "requestTime"   : {"type": "double"},
                "time"          : {"type": "date"},
                "cache"         : {"type": "boolean"},
                "uri"           : {"type": "text"},
                "level"         : {"type": "keyword"},
                "message"       : {"type": "text"},
                "host"          : {"type": "keyword"},
                "cpu": {
                    "dynamic": true,
                    "properties": {
                        "system"    : {"type": "long"},
                        "user"      : {"type": "long"}
                    }
                },
                "memory": {
                    "dynamic": true,
                    "properties": {
                        "external"  : {"type": "long"},
                        "heapTotal" : {"type": "long"},
                        "rss"       : {"type": "long"},
                        "heapUsed"  : {"type": "long"}
                    }
                }
            }
        }
    },
    "settings": {
        "index.refresh_interval": "5s"
    },
    "index_patterns": "graphql-*",
    "version": 50001
}